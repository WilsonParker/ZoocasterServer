var nowLocation;

function log(msg) {
	console.log(msg);
};

function movePage(page) {
	nowLocation = page;
	$.ajax({
		url : page,
		success : function(result){
			$("#content").html(result);
		}
	})
};

function moveLocation(page) {
	window.location.href = page;
};

function reload(){
	movePage(nowLocation);
};






