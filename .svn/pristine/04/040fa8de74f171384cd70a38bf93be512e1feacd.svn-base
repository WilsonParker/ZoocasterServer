package com.graction.lumi.model.weather;

public class Wind {
	private double speed, deg;	// Wind speed, Unit Default: meter/sec, Metricr: meer/sec, Imperial: miles/hour
								// Wind direction, degrees (meteorological)

	@Override
	public String toString() {
		return "Wind [speed=" + speed + ", deg=" + deg + "]";
	}


	public double getSpeed() {
		return speed;
	}


	public void setSpeed(double speed) {
		this.speed = speed;
	}


	public double getDeg() {
		return deg;
	}


	public void setDeg(double deg) {
		this.deg = deg;
	}

}
